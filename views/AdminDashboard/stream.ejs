<!DOCTYPE html>
<html lang="en">
  <head>
    <% include partials/head%>
    <title>Apollo Hospitals LiveStream</title>
  </head>

  <body id="page-top">
    <!-- Page Wrapper -->
    <div id="wrapper">
      <!-- Sidebar -->
      <% include partials/sidebar%>
      <!-- End of Sidebar -->

      <!-- Content Wrapper -->
      <div id="content-wrapper" class="d-flex flex-column">
        <!-- Main Content -->
        <div id="content">
          <!-- Topbar -->
          <% include partials/topbar%>
          <!-- End of Topbar -->

          <!-- Begin Page Content -->
          <div class="container-fluid">
            <!-- Page Heading -->
            <div
              class="d-sm-flex align-items-center justify-content-between mb-4"
            >
              <h1 class="h3 mb-0 text-gray-800">
                Live Feed: TODO patient name
              </h1>
            </div>

            <!-- Content Row -->

            <div class="row">
              <div class="card-body" id="videoAttachment">
                <script type="text/JavaScript">
                  var playerInstance = jwplayer("videoAttachment");
                  playerInstance.setup({
                      file: "http://10.0.15.152:8080/playlist.m3u8",
                      autostart: 'viewable',
                      width: 1280,
                      height: 600
                  });
                </script>
              </div>

              <!-- Pie Chart -->
            </div>
          </div>
          <!-- /.container-fluid -->
        </div>
        <!-- End of Main Content -->

        <!-- Footer -->
        <footer class="sticky-footer bg-white">
          <div class="container my-auto">
            <div class="copyright text-center my-auto">
              <span>Double Helix 2019</span>
            </div>
          </div>
        </footer>
        <!-- End of Footer -->
      </div>
      <!-- End of Content Wrapper -->
    </div>
    <!-- End of Page Wrapper -->

    <!-- Scroll to Top Button-->
    <% include partials/footer %>
  </body>
</html>
